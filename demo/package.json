{
  "name": "living-docs-demo",
  "version": "1.0.0",
  "description": "Demo project showcasing Living Documentation Generator capabilities",
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../python-service && pip install -r requirements.txt",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"npm run python-service\" \"npm run go-service\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm start",
    "python-service": "cd python-service && python app.py",
    "go-service": "cd go-service && go run main.go",
    "docs:serve": "node ../dist/server.js --config demo-config.json",
    "docs:generate": "node ../dist/server.js --generate --config demo-config.json",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "benchmark": "node tests/performance/benchmark.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "jest": "^29.5.0",
    "@playwright/test": "^1.35.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0"
  },
  "keywords": ["documentation", "mcp", "kiro", "demo"],
  "author": "Kiro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgharlow/kiro-living-docs-devpost.git"
  },
  "bugs": {
    "url": "https://github.com/sgharlow/kiro-living-docs-devpost/issues"
  },
  "homepage": "https://github.com/sgharlow/kiro-living-docs-devpost#readme"
}